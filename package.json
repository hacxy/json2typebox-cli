{
  "name": "json2typebox-cli",
  "type": "module",
  "version": "0.1.0",
  "description": "Json to typebox schemas cli.",
  "author": {
    "name": "hacxy",
    "email": "hacxy.js@outlook.com",
    "url": "https://github.com/hacxy"
  },
  "license": "MIT",
  "homepage": "https://github.com/hacxy/json2typebox-cli#readme",
  "bugs": {
    "url": "https://github.com/hacxy/json2typebox-cli/issues"
  },
  "keywords": [
    "json2typebox",
    "json2typebox-cli",
    "cli"
  ],
  "bin": {
    "json2typebox": "./index.js",
    "jtt": "./index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node dist/index.mjs",
    "dev": "unbuild --stub",
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "release": "bump",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "eslint --fix"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "clipboardy": "^4.0.0",
    "json2typebox": "^0.0.3",
    "log-symbols": "^7.0.0"
  },
  "devDependencies": {
    "@hacxy/bump": "^0.1.0",
    "@hacxy/eslint-config": "^0.4.1",
    "@types/node": "^20.17.0",
    "eslint": "^8.57.0",
    "lint-staged": "^16.1.2",
    "simple-git-hooks": "^2.13.0",
    "typescript": "^5.4.4",
    "unbuild": "^2.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "node scripts/commit-msg.mjs"
  },
  "lint-staged": {
    "*": [
      "npm run lint:staged"
    ]
  }
}
